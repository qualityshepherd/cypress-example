{
  "name": "cypress-example",
  "version": "1.0.0",
  "description": "An example Cypress e2e test project",
  "author": "brine",
  "license": "MIT",
  "keywords": [
    "cypress",
    "e2e",
    "ui",
    "test",
    "automation"
  ],
  "standard": {
    "globals": [
      "cy",
      "beforeAll",
      "afterAll",
      "describe",
      "it",
      "beforeEach",
      "afterEach"
    ]
  },
  "scripts": {
    "cy": "cypress",
    "open": "npm run cy open",
    "test": "npm run cy run -- -q --headed",
    "headless": "npm run cy run -- -q",
    "home": "npm run cy run -- --spec specs/homePageSpec.js",
    "pretty": "standard --fix",
    "ci": "npm run cy run -- -q"
  },
  "dependencies": {
    "cypress": "^12.17.0",
    "standard": "^17.1.0"
  }
}
